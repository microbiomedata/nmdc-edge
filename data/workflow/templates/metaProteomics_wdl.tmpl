            MASIC_PARAM_FILE_LOC=<WORKFLOW>_MASIC_PARAM_FILE_LOC,
            MSGFPLUS_PARAM_FILE_LOC=<WORKFLOW>_MSGFPLUS_PARAM_FILE_LOC,
            CONTAMINANT_FILE_LOC=<WORKFLOW>_CONTAMINANT_FILE_LOC,
            QVALUE_THRESHOLD=<WORKFLOW>_QVALUE_THRESHOLD,
            STUDY=<WORKFLOW>_STUDY,
            EXECUTION_RESOURCE=<WORKFLOW>_EXECUTION_RESOURCE,
            OUTDIR=<WORKFLOW>_OUTDIR
    }
