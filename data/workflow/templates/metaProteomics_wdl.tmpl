    input {
        Array[Object] <WORKFLOW>_mapper_list
        String <WORKFLOW>_DAT_URL
        File <WORKFLOW>_MASIC_PARAM_FILE_LOC
        File <WORKFLOW>_MSGFPLUS_PARAM_FILE_LOC
        File <WORKFLOW>_CONTAMINANT_FILE_LOC
        Float <WORKFLOW>_QVALUE_THRESHOLD
        String <WORKFLOW>_STUDY
        String <WORKFLOW>_EXECUTION_RESOURCE
        String <WORKFLOW>_OUTDIR
    }

    call <WORKFLOW>.metapro as <ALIAS> {
        input: 
            mapper_list=<WORKFLOW>_mapper_list,
